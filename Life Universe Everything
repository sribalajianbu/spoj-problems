package programming;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		System.out.println("Enter the input");
		Scanner sc = new Scanner(System.in);
		while(true)
		{
		int a = sc.nextInt();
		if(a==42)
		{
			break;
		}
		else
			System.out.println(a);
}
}
}
